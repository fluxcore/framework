<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Rewrite to index.php">
                	<match url="index.php|robots.txt|images|test.php"/>
                	<action type="None"/>
                </rule>
                <rule name="Rewrite to FluxCore">
                    <match url=".*"/>
                    <conditions>
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true"/>
                    </conditions>
                    <action type="Rewrite" url="index.php?p={R:0}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
